						<ButtonText parentKey="Text" inherits="GameFontNormal" text=" /w " setAllPoints="true" />
						<Scripts>
							<OnLoad>
								local info = ChatTypeInfo["WHISPER"];
								self.Text:SetTextColor(info.r, info.g, info.b);
								self:SetWidth(self.Text:GetStringWidth());
