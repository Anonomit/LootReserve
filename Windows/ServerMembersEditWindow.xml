					</OnLoad>
					<OnClick>
						if LootReserve.Server.CurrentSession then
							LootReserve.Server.Export.Window:SetShown(not LootReserve.Server.Export.Window:IsShown());
							return;
						end

						local menu =
						{
							{ text = "Import from CSV", disabled = LootReserve.Server.CurrentSession ~= nil, func = function() LootReserve.Server.Import.Window:Show(); end },
							{ text = "Export to CSV", func = function() LootReserve.Server.Export.Window:Show(); end },
							{ text = "Clear Imported Data", disabled = LootReserve.Server.CurrentSession or not next(LootReserve.Server.NewSessionSettings.ImportedMembers), func = function() LootReserve.Server.MembersEdit:ClearImported(); end },
							LootReserve:MakeMenuSeparator(),
